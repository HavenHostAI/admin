{
  "name": "admin-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "openapi:lint": "spectral lint openapi/openapi.yaml --ruleset openapi/.spectral.yaml",
    "openapi:bundle": "redocly bundle openapi/openapi.yaml -o openapi/dist.yaml",
    "openapi:types": "openapi-typescript openapi/dist.yaml -o src/types/openapi.d.ts",
    "openapi:docs": "redocly build-docs openapi/dist.yaml -o docs/api",
    "openapi:validate": "pnpm openapi:lint && pnpm openapi:bundle && pnpm openapi:types",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "pretest:e2e": "playwright install --with-deps chromium",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
